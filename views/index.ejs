<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Permanent+Marker&family=Roboto:wght@100&display=swap" rel="stylesheet">

    <title>hodlinfo</title>
</head>

<body>
    <div class="app">
        <div class="container-fluid">
            <div class="row align-items-center justify-content-between">
                <div class="col-6 col-md-3">
                    <img class="img-title" src="/images/title.png" alt="title" width="360px" height="100px">
                </div>
                <div class="col-6 col-md-2 text-end text-center">
                    <div class="dropdown show btn-grp">
                        <a class="btn btn-secondary dropdown-toggle button1" href="#" role="button"
                            id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            INR
                        </a>

                        <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                            <a class="dropdown-item" href="#">INR</a>
                        </div>

                        <div class="crypto-dropdown">
                            <a class="btn btn-secondary dropdown-toggle button2" href="#" role="button"
                                id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                BTC
                            </a>

                            <div class="dropdown-menu" aria-labelledby="dropdownMenuLink" >
                                <a value="BTC" tabindex="0" role="menuitem" class="dropdown-item"
                                    href="/BTC-INR">BTC</a>
                                <a value="ETH" tabindex="0" role="menuitem" class="dropdown-item"
                                    href="/ETH-INR">ETH</a>
                                <a value="USDT" tabindex="0" role="menuitem" class="dropdown-item"
                                    href="/USDT-INR">USDT</a>
                                <a value="XRP" tabindex="0" role="menuitem" class="dropdown-item"
                                    href="/XRP-INR">XRP</a>
                                <a value="TRX" tabindex="0" role="menuitem" class="dropdown-item"
                                    href="/TRX-INR">TRX</a>
                                <a value="DASH" tabindex="0" role="menuitem" class="dropdown-item"
                                    href="/DASH-INR">DASH</a>
                                <a value="ZEC" tabindex="0" role="menuitem" class="dropdown-item"
                                    href="/ZEC-INR">ZEC</a>
                                <a value="XEM" tabindex="0" role="menuitem" class="dropdown-item"
                                    href="/XEM-INR">XEM</a>
                                <a value="IOST" tabindex="0" role="menuitem" class="dropdown-item"
                                    href="/IOST-INR">IOST</a>
                                <a value="WIN" tabindex="0" role="menuitem" class="dropdown-item"
                                    href="/WIN-INR">WIN</a>
                                <a value="BTT" tabindex="0" role="menuitem" class="dropdown-item"
                                    href="/BTT-INR">BTT</a>
                                <a value="WRX" tabindex="0" role="menuitem" class="dropdown-item"
                                    href="/WRX-INR">WRX</a>
                            </div>
                        </div>

                        <a class="btn btn-secondary button3" href="#" role="button" aria-haspopup="true"
                            aria-expanded="false">
                            BUY BTC
                        </a>
                    </div>

                    <div class="right-header col-12 col-sm-12 col-md-5 col-lg-4">
                        <div class="d-flex align-items-center justify-content-center">
                            <div class="timer-circle ">
                                <div class="timer" id="countdown">60</div>
                            </div>
                            <a href="connect/telegram" class="btn btn-primary btn-telegram">
                                <div class="d-flex align-items-center justify-content-center">
                                    <img src="data:image/png;base64,"
                                        alt="" width="18px" height="18px"> <span>Connect Telegram</span>
                                </div>
                            </a>

                            <div class="margin-10 d-inline-block">
                                <label class="switch">
                                    <input type="checkbox" checked>
                                    <span class="slider round"></span>
                                </label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div>
                <img class="detail-img" src="/images/details-title.png" alt="" srcset="" style="max-width: 100%;">
            </div>

            <div class="fiat-crypto-table table-responsive">
                <table class="table table-borderless">
                    <thead>
                        <tr>
                            <th>
                                <h4>
                                    <span class="pointer">#</span>
                                </h4>
                            </th>
                            <th>
                                <h4>
                                    <span class="pointer">Platform</span>
                                </h4>
                            </th>
                            <th>
                                <h4>
                                    <span class="pointer">Last Traded Price</span>
                                </h4>
                            </th>
                            <th>
                                <h4>
                                    <span class="pointer">Buy / Sell Price</span>
                                </h4>
                            </th>
                            <th>
                                <h4>
                                    <span class="pointer">Volume</span>
                                </h4>
                            </th>
                            <th>
                                <h4>
                                    <span class="pointer">Base Unit</span>
                                </h4>
                            </th>
                        </tr>
                    </thead>

                    <tbody>
                        <% details.forEach((detail, index) => { %>
                            <tr class="rounded" style="border-radius: 10px;">
                              <td class="align-middle"><h4><%= index + 1 %></h4></td>
                              <td class="align-middle"><h4><%= detail.name %></h4></td>
                              <td class="align-middle"><h4><%= detail.last %></h4></td>
                              <td class="align-middle"><h4><%= detail.buy %> / <%= detail.sell %></h4></td>
                              <td class="align-middle"><h4><%= detail.volume %></h4></td>
                              <td class="align-middle"><h4><%= detail.base_unit %></h4></td>
                            </tr>
                          <% }); %>
                    </tbody>
                </table>
            </div>
            <div class="footer d-flex justify-content-between align-items-center">
                <div class="d-flex">
                    <div class="footer-text">
                        Â© 2023 CryptoTejas
                    </div>
                </div>
                <div class="footer-text">
                    Support
                </div>
            </div> 

            
        </div>
       
    </div>

    <script>
        const dropdownItems = document.querySelectorAll('.dropdown-menu a');
        const buyButton = document.querySelector('.button2');

        dropdownItems.forEach(item => {
            item.addEventListener('click', () => {
                const currency = item.getAttribute('value');
                buyButton.textContent = `BUY ${currency}`;
            });
        });


        var countDownDate = new Date().getTime() + 60000;

        var x = setInterval(function () {
            var now = new Date().getTime();

            var distance = countDownDate - now;

            var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
            var seconds = Math.floor((distance % (1000 * 60)) / 1000);

            if (seconds < 10) {
                seconds = "0" + seconds;
            }

            document.getElementById("countdown").innerHTML = seconds;

            if (distance < 0) {
                clearInterval(x);
                location.reload();
            }
        }, 1000);
    </script>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
        integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"
        integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"
        integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
        crossorigin="anonymous"></script>
</body>

</html>